{
   "name": "Cacodemon Game",
   "description": "An extension that replaces the normal Chrome Dino Game with an alternative game where you play as a cacodemon avoiding rockets.",
   "version": "1.0",
   "manifest_version": 3,
   "default_locale": "en",
   "icons": {
       "16": "/images/icons/icon16.png",
       "48": "/images/icons/icon48.png",
       "128": "/images/icons/icon128.png"
   },
   "page_action": {
       "default_icon": {
           "16": "/images/icons/icon16.png",
           "48": "/images/icons/icon48.png",
           "128": "/images/icons/icon128.png"
       }
   },
   "content_scripts": [{
       "matches": ["chrome://dino/"],
       "html": ["index.html"],
       "css": ["main.css"],
       "js": ["script.js"]
   }]
}


